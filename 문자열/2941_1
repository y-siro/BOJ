#include <iostream>
#include <string>
#include <vector>
using namespace std;

int main(void) {
	vector<string> s= { "c=","c-","dz=","d-","lj","nj","s=","z=" };
	int index;
	string str;
	cin >> str;
	for (int i = 0; i < s.size(); i++) {
		while (1) {
			index = str.find(s[i]);
			if (index == string::npos) break;
			str.replace(index, s[i].length(), "@");
		}
	}
	cout << str.length() << endl;
	return 0;
}
